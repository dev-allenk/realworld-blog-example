{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@types": "./src/@types",
          "@api": "./src/api",
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@hooks": "./src/hooks",
          "@modules": "./src/modules"
        }
      }
    ]
  ]
}
